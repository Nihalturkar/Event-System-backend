{
  "name": "photomatch-backend",
  "version": "1.0.0",
  "description": "PhotoMatch - AI-Powered Wedding Photo Sharing Backend",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "postinstall": "node -e \"try{require('@tensorflow/tfjs-node')}catch(e){const fs=require('fs'),p='node_modules/@tensorflow/tfjs-node';fs.mkdirSync(p,{recursive:true});fs.writeFileSync(p+'/index.js','module.exports=require(\\\"@tensorflow/tfjs\\\");');fs.writeFileSync(p+'/package.json','{\\\"name\\\":\\\"@tensorflow/tfjs-node\\\",\\\"version\\\":\\\"4.22.0\\\",\\\"main\\\":\\\"index.js\\\"}');console.log('Created tfjs-node shim')}\""
  },
  "dependencies": {
    "@tensorflow/tfjs": "^4.22.0",
    "@vladmandic/face-api": "^1.7.15",
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.41.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jimp": "^0.22.10",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "qrcode": "^1.5.3",
    "sharp": "^0.33.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.0"
  }
}
